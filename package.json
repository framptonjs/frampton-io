{
  "name": "frampton-io",
  "version": "0.1.2",
  "description": "A module for managing browser io with frampton.js",
  "scripts": {
    "test": "frampton test",
    "build": "frampton build",
    "clean-install": "rm -rf node_modules; npm install;",
    "release": "frampton release --patch",
    "minor-release": "frampton release --minor",
    "major-release": "frampton release --major",
    "docs": "rm -rf docs/* && jsdoc packages/**/src/*.js -d docs --readme README.md  --private"
  },
  "author": "Kevin Greene <hightimesteddy@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:framptonjs/frampton-io.git"
  },
  "bugs": {
    "url": "https://github.com/framptonjs/frampton-io/issues"
  },
  "homepage": "https://github.com/framptonjs/frampton-io",
  "devDependencies": {
    "frampton": "0.2.3",
    "frampton-build": "0.0.6",
    "frampton-cli": "0.0.10",
    "jsdoc": "3.4.0"
  }
}
